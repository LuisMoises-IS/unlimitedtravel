      <h1 class="h3 mb-3 fw-normal">Log in</h1>

      <% if (inputData.hasError) { %>
        <p id="input-error">
          <%= inputData.message %>
        </p>
        <% } %>

          <div class="form-floating">
            <input type="email" class="form-control" id="email" name="email" value="<%= inputData.email %>" required>
            <label for="correo">Correo</label>
          </div>
          <div class="form-floating">
            <input type="password" class="form-control" id="password" name="password" value="<%= inputData.password %>"
              required>
            <label for="pass">Password</label>
          </div>

          <div class="checkbox mb-3">
            <label>
              <input type="checkbox" value="remember-me"> Remember me
            </label>
          </div>
          <button class="w-100 btn btn-lg btn-success" type="submit">Log in</button>
    </form>
    <br>
    <a href="/signup" type="button" class="w-100 btn btn-lg btn-success">Registrarse</a>

