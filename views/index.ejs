            <p><a class="btn btn-lg btn-success" href="#">Ver mas</a></p>
          </div>
        </div>
      </div>

      <div class="carousel-item">
        <svg class="bd-placeholder-img" width="100%" height="100%" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"
          preserveAspectRatio="xMidYMid slice" focusable="false">
          <rect width="100%" height="100%" fill="#777" />
        </svg>

        <div class="container">
          <div class="carousel-caption">
            <h1>Anuncio 3</h1>
            <p>Informacion de anuncio 3</p>
            <p><a class="btn btn-lg btn-success" href="#">Ver mas</a></p>
          </div>
        </div>
      </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#myCarousel" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#myCarousel" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>

  <div>
    <h1 style="text-align: center;">Ultimos vehiculos añadidos</h1>
  </div>

  <!-- Card para los vehiculos -->


  <section style="padding: 2rem 4rem 0 4rem;">

    <div class="row row-cols-1 row-cols-md-3 g-4">
      <% i=0; %>
        <% for (const vehiculo of vehiculos) {%>
          <% if(i>= (vehiculos.length - 6)) { %>
            <div class="col">
              <div class="card h-100">
                <div>
                  <img style="object-fit: cover; width: 28rem; height: 18rem;" src="<%= vehiculo.imagePath %> "
                    class="card-img-top" alt="Imagen del vehiculo.">
                </div>
                <div class="card-body">
                  <h4 class="card-title">
                    <%= vehiculo.marca %> - <%= vehiculo.model %>
                  </h4>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="btn-group">
                      <a type="button" class="btn btn-sm btn-success" href="/vehiculos/<%= vehiculo._id %> ">Ver
                        Vehiculo</a>
                      <a type="button" class="btn btn-sm btn-outline-success"
                        href="/vehiculos/<%= vehiculo._id %>/reserva">Reserva</a>
                    </div>
                    <small class="text-muted">Precio: $<%= vehiculo.precio %></small>
                  </div>
                </div>
              </div>
            </div>
            <% } %>
              <% i++; %>
                <% } %>


    </div>
  </section>





  <section class="py-5 text-center container">
    <div class="row py-lg-5" style="background-color: rgb(154, 204, 177);">
      <div class="col-lg-6 col-md-8 mx-auto">
        <h1 class="fw-light">Plan corporativo</h1>
        <p class="lead text-muted">Añadir descripcion del plan corporativo orientado a empresas</p>
        <p>
          <a href="#" class="btn btn-primary my-2">Ver mas</a>

        </p>
      </div>
    </div>
  </section>


  <div>
    <h1 style="text-align: center;">Nuestros servicios</h1>
  </div>

  <!-- Marketing messaging and featurettes
  ================================================== -->
  <!-- Wrap the rest of the page in another container to center all the content. -->

  <div class="container marketing">

    <!-- Three columns of text below the carousel -->
    <div class="row">

      <% for (const servicio of servicios ) {%>
        <div class="col-lg-4">
          <img src="<%=servicio.imagen %>" class="d-block mx-lg-auto img-fluid" alt="Bootstrap Themes" width="200"
            height="200" loading="lazy" style="border-color: black; border-radius: 55%;">

          <h2>
            <%=servicio.nombre %>
          </h2>
          <p><a class="btn btn-outline-success" href="/servicios/<%= servicio._id %> ">Mas Informacion &raquo;</a></p>
        </div><!-- /.col-lg-4 -->
        <% } %>




    </div><!-- /.row -->



    <footer class="container">

      <div>
        <p class="float-center "><a href="#">Back to top</a></p>
        <p style="text-align: center;"><a href="#">Acerca de nosotros</a> &middot; <a href="#">Contacto</a> &middot; <a
            href="#">Ubicaciones</a> </p>
        <p style="text-align: center;">&copy; 2022 Nombre de la empresa.</p>
      </div>
    </footer>





