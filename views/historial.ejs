						<table class="table"> 
							<thead class="thead-dark"> 
								<tr>
                                    <th scope="col">Fecha</th>
                                    <th scope="col">Vehiculo</th>
                                    <th scope="col">Lugar Retiro</th>
                                    <th scope="col">Lugar Entrega</th>
                                    <th scope="col">Estado</th>
                                    <th scope="col"> BOTON</th>
                                </tr>
                            </thead>
                        <tbody>
                            <% for (const reserva of reservas ) {%>
                             <tr>
                                <td ><%= reserva.fecha %>  </td>
                                <td ><%= reserva.vehiculo %> </td>
                                <td ><%= reserva.retiro %> </td>
                                <td ><%= reserva.entrega %> </td>
                                <td ><%= reserva.estado %> </td>
                                <td ><a type="button" class="btn btn-sm btn-outline-success" href="/historial-detalle/<%=reserva._id%>">Detalles</a></td>
                                
                    </div>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>

        
    <div class="b-example-divider"></div>

</main>
</section>

